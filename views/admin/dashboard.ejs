<!-- dynamic dashboard -->


<%- include ('../layout/header') %>



<main id="wrapper" class="d-flex">
  <!-- SIDEBAR -->
  <nav id="sidebarMenu">
    <div
      class="sidebar-header d-flex justify-content-between aligh-item-center"
    >
      <h3>MillionsClub</h3>
      <button type="button" id="closeSidebar" class="btn btn-light">
        <i class="bi bi-x-lg"></i>
      </button>
    </div>
    <ul class="list-unstyled components">
      <li><a href="/admin/dashboard" data-page="dashboard">Dashboard</a></li>
      <li><a href="/admin/catalog" data-page="catalog">Catalog</a></li>
      <li><a href="/admin/customers" data-page="customers">Customers</a></li>
      <li><a href="/admin/orders" data-page="orders">Orders</a></li>
      <li><a href="/admin/settings" data-page="settings">Settings</a></li>
    </ul>
  </nav>

  <div class="container" id="container">
    <!-- Navbar -->
    <nav class="navbar navbar-light bg-light shadow-sm">
      <div class="container-fluid">
        <button class="navbar-toggler" type="button" id="hamburger">
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="d-flex">
          <button class="btn btn-light" type="button">
            <i class="bi bi-bell"></i>
          </button>

          <div class="dropdown">
            <button
              class="btn btn-light dropdown-toggle"
              type="button"
              id="profileDropdown"
              data-bs-toggle="dropdown"
              aria-expanded="false"
            >
              <i class="bi bi-person"></i>
            </button>
            <ul
              class="dropdown-menu dropdown-menu-end"
              aria-expanded="profileDropdown"
            >
              <li>
                <a class="dropdown-item" href="/admin/profile">Profile</a>
              </li>
              <li><a class="dropdown-item" href="/admin/logout">Logout</a></li>
            </ul>
          </div>
        </div>
      </div>
    </nav>

    

    <!-- Dashboard content -->
    <div class="container" id="dashboardContent">

   <!-- breadcrumbs -->
   <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
      <% if (breadcrumb && breadcrumb.length) { %>
        <% breadcrumb.forEach((crumb, index) => { %>
          <% if (index === breadcrumb.length - 1) { %>
            <li class="breadcrumb-item active" aria-current="page"><%= crumb.name %></li>
          <% } else { %>
            <li class="breadcrumb-item"><a href="<%= crumb.url %>"><%= crumb.name %></a></li>
          <% } %>
        <% }); %>
      <% } %>
    </ol>
  </nav>

     
      
  <h2 class="my-5">Dashboard Overview</h2>

  <!-- first row -->
  <div class="row">
    <div class="col m-2 m-sm-0">
      <div class="card bg-success text-white">
        <div class="card-body">
          <i class="fa-solid fa-cart-shopping"></i>
          <h5 class="card-title">Today Orders</h5>
          <% if(typeof todayOrders !== 'undefined') {%> 
          <p class="card-text">₹ <%= todayOrders.totalSales %> (Orders: <%= todayOrders.orderCount %>)</p>
          <%}else{%>
            No today Orders
            <%}%>
        </div>
      </div>
    </div>
  
    <div class="col m-2 m-sm-0">
      <div class="card bg-warning text-white">
        <div class="card-body">
          <i class="fa-solid fa-cart-shopping"></i>
          <h5 class="card-title">Yesterday Orders</h5>
          <% if(typeof yesterdayOrders !== 'undefined') {%> 
          <p class="card-text">₹ <%= yesterdayOrders.totalSales %> (Orders: <%= yesterdayOrders.orderCount %>)</p>
          <%}else{%>
            No yesterday Orders
            <%}%>

        </div>
      </div>
    </div>
  
    <div class="col m-2 m-sm-0">
      <div class="card bg-primary text-white">
        <div class="card-body">
          <i class="fa-solid fa-calendar-days"></i>
          <h5 class="card-title">This Month</h5>
          <% if(typeof monthOrders !== 'undefined') {%> 
          <p class="card-text">₹ <%= monthOrders.totalSales %> (Orders: <%= monthOrders.orderCount %>)</p>
          <%}else{%>
            No month Orders
            <%}%>

        </div>
      </div>
    </div>
  
    <div class="col m-2 m-sm-0">
      <div class="card bg-info text-white">
        <div class="card-body">
          <i class="fa-regular fa-calendar"></i>
          <h5 class="card-title">This Year</h5>
          <% if(typeof yearOrders !== 'undefined') {%> 
          <p class="card-text">₹ <%= yearOrders.totalSales %> (Orders: <%= yearOrders.orderCount %>)</p>

   <%}else{%>
    No year Orders
    <%}%>

        </div>
      </div>
    </div>
  
    <div class="col m-2 m-md-0">
      <div class="card bg-success text-white">
        <div class="card-body">
          <i class="fa-solid fa-wallet"></i>
          <h5 class="card-title">Total Sales</h5>
          <% if(typeof totalSales !== 'undefined') {%> 
          <p class="card-text">₹ <%= totalSales.totalSales %> (Orders: <%= totalSales.orderCount %>)</p>
          <%}else{%>
            No total 
            <%}%>


        </div>
      </div>
    </div>
  </div>
  
  <!-- second row -->
  <div class="row g-3 my-3">
    <div class="col-md-3">
      <div class="card custom-card">
        <div class="card-body custom-card-body">
          <div class="icon-bg bg-primary bg-opacity-25 custom-icon">
            <i class="fas fa-shopping-cart text-warning"></i>
          </div>
          <div>
            <h5 class="card-title custom-card-title pt-3">Total Orders</h5>

            <% if(typeof totalOrders !== 'undefined') {%> 

            <p class="card-text"><%= totalOrders %></p>

            <%}else{%>
              No total Orders
              <%}%>
          </div>
        </div>
      </div>
    </div>
  
    <div class="col-md-3">
      <div class="card">
        <div class="card-body custom-card-body">
          <div class="icon-bg bg-primary bg-opacity-25 custom-icon">
            <i class="fas fa-sync-alt text-primary"></i>
          </div>
          <div>
            <h5 class="card-title custom-card-title pt-3">Orders Pending</h5>
            <p class="card-text">
              <% if(typeof pendingOrders !== 'undefined') {%>
              <%= pendingOrders[0]?.orderCount || 0 %> <span class="pending-amount">(₹ <%= pendingOrders[0]?.totalAmount || 0 %>)</span>
            </p>
            <%}else{%>
            NA
              <%}%>
  
          </div>
        </div>
      </div>
    </div>
  
    <div class="col-md-3">
      <div class="card">
        <div class="card-body custom-card-body">
          <div class="icon-bg bg-info bg-opacity-25 custom-icon">
            <i class="fas fa-truck text-info"></i>
          </div>
          <div>
            <h5 class="card-title custom-card-title pt-3">Orders Processing</h5>
            <% if(typeof processingOrders !== 'undefined' && processingOrders !== null) { %>
              <p class="card-text"><%= processingOrders %></p>
            <% } else { %>
              <p class="card-text">No processing orders</p>
            <% } %>
          </div>
        </div>
      </div>
    </div>
  
    <div class="col-md-3">
      <div class="card">
        <div class="card-body custom-card-body">
          <div class="icon-bg bg-success bg-opacity-25 custom-icon">
            <i class="fas fa-check text-success"></i>
          </div>
          <div>
            <h5 class="card-title custom-card-title pt-3">Orders Delivered</h5>
            <p class="card-text">60</p>
          </div>
        </div>
      </div>
    </div>
  </div>
  
  <!-- Recent orders table in dashboard -->
  <h4 class="my-5">Recent Orders</h4>
  
  <div class="table-responsive-sm">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">INVOICE NUMBER</th>
          <th scope="col">QUANTITY</th>
          <th scope="col">ORDER TIME</th>
          <th scope="col">CUSTOMER NAME</th>
          <th scope="col">AMOUNT</th>
          <th scope="col">METHOD</th>
          <th scope="col">STATUS</th>
          <th scope="col">ACTION</th>
        </tr>
      </thead>
      <tbody>
        <% if(typeof recentOrders !== 'undefined' && recentOrders.length > 0) { %>
          <% recentOrders.forEach(order => { %>
            <tr>
              <td><%= order._id %></td>
              <td><%= order.totalQuantity %></td>
              <td><%= order.createdAt.toLocaleString() %></td>
              <td><%= order.customerName %></td>
              <td>₹ <%= order.totalPrice %></td>
              <td><%= order.paymentMethod %></td>
              <td><%= order.deliveryStatus %></td>
              <td>
                <div class="btn-group">
                  <button type="button" class="btn btn-danger">Cancel</button>
                  <button type="button" class="btn btn-danger dropdown-toggle dropdown-toggle-split" data-bs-toggle="dropdown" aria-expanded="false">
                    <span class="sr-only">Toggle Dropdown</span>
                  </button>
                  <div class="dropdown-menu">
                    <a class="dropdown-item" href="">Change status</a>
                    <a class="dropdown-item" href="">Return</a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="#">Separated link</a>
                  </div>
                </div>
              </td>
            </tr>
          <% }) %>
        <% } else { %>
          <tr>
            <td colspan="8" class="text-center">No recent orders available</td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  </div>
    <!-- end -->

    <!-- catalog tab content -->
    <div class="container" id="productsContent">

     <!-- breadcrumbs -->
     <nav aria-label="breadcrumb" class="my-3">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
        <% if (typeof breadcrumb !== 'undefined' && breadcrumb && breadcrumb.length > 0) { %>
          <% breadcrumb.forEach((crumb, index) => { %>
            <% if (index === breadcrumb.length - 1) { %>
              <li class="breadcrumb-item active" aria-current="page"><%= crumb.name %></li>
            <% } else { %>
              <li class="breadcrumb-item"><a href="<%= crumb.url %>"><%= crumb.name %></a></li>
            <% } %>
          <% }); %>
        <% } %>
      </ol>
    </nav>

      <h4 class="my-5">Products Catalog</h4>

      <form action="/admin/catalog" method="GET">
        <div class="row">
          <div class="col-6">
            <div class="input-group mb-3">
              <input name="search" type="text" class="form-control"
              placeholder="Search..." aria-label="Search" <% if(typeof
              searchQuery !== 'undefined'){ %> value="<%= searchQuery %>" <% }
              %> />
              <button class="btn btn-outline-secondary" type="submit">
                Search
              </button>
            </div>
          </div>
          <div class="col-4 text-end">
            <a href="/admin/add-product" class="btn btn-primary">Add Product</a>
          </div>
        </div>
      </form>

      <div class="table-responsive">
        <table class="table table-bordered">
          <thead>
            <tr>
              <th>Barcode</th>
              <th>Images</th>
              <th>Product Name</th>
              <th>SKU</th>
              <th>Category</th>
              <th>Sub Category</th>
              <th>Brand</th>
              <th>Cost</th>
              <th>Discount Percentage %</th>
              <th>Discount Valid Until</th>
              <th>Stock (Size)</th>
              <th>Stock (Color)</th>
              <th>Stock (Quantity)</th>
              <th>Regular Price</th>
              <th>Sale Price</th>
              <th>Created Date</th>
              <th>Updated</th>
              <th scope="col" class="align-middle">ACTION</th>
            </tr>
          </thead>
          <tbody>
            <% if (typeof product !== 'undefined' && product.length > 0) { %> <%
            product.forEach((item) => { %>
            <tr>
              <td class="align-middle"><%= item.barcode %></td>

              <td class="align-middle">
                <% if (typeof item.photos !== 'undefined' && item.photos.length
                > 0) { %>
                <div class="image-grid">
                  <% item.photos.forEach((photo) => { %>
                  <div
                    class="image-wrapper border border-dark-subtle border-2 p-1"
                  >
                    <img class="image-item" src="<%= photo %>" alt="image" />
                  </div>
                  <% }) %>
                </div>
                <% } else { %> No images available <% } %>
              </td>

              <td class="align-middle"><%= item.productName %></td>
              <td class="align-middle"><%= item.SKU %></td>
              <td class="align-middle"><%= item.category %></td>
              <td class="align-middle"><%= item.subCategory %></td>
              <td class="align-middle"><%= item.brand %></td>
              <td class="align-middle"><%= item.cost %></td>
              <td class="align-middle">
                <%= item.discount.percentage || 'N/A' %>
              </td>
              <td class="align-middle">
                <%= item.discount.valid_until || 'N/A' %>
              </td>
              <td class="align-middle">
                <% item.stock.forEach((stock)=>{%> <%= stock.size%><%})%>
              </td>
              <td class="align-middle">
                <% item.stock.forEach((stock)=>{%> <%= stock.color%><%})%>
              </td>
              <td class="align-middle">
                <% item.stock.forEach((stock)=>{%> <%= stock.quantity%><%})%>
              </td>
              <td class="align-middle">
                <% item.stock.forEach((stock)=>{%> <%=
                stock.price.regularPrice%><%})%>
              </td>
              <td class="align-middle">1399</td>
              <td class="align-middle"><%= item.createdDate %></td>
              <td class="align-middle"><%= item.updated %></td>
              <td class="align-middle">
                <div class="btn-group">
                  <a
                    class="btn btn-danger"
                    data-id="<%= item._id%>"
                    href="/admin/edit-product?Id=<%= item._id%>"
                    >Edit</a
                  >
                  <button
                    type="button"
                    class="btn btn-danger dropdown-toggle dropdown-toggle-split"
                    data-bs-toggle="dropdown"
                    aria-haspopup="true"
                    aria-expanded="false"
                  >
                    <span class="sr-only">Toggle Dropdown</span>
                  </button>
                  <div class="dropdown-menu">
                    <a class="dropdown-item" href="#">Change status</a>
                    <button
                      class="dropdown-item delete-btn"
                      data-id="<%= item._id%>"
                    >
                      Delete
                    </button>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="#">Separated link</a>
                  </div>
                </div>
              </td>
            </tr>
            <% }) %> <% } else { %>
            <tr>
              <td colspan="18">No products available</td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      <!-- Pagination -->

<% if (typeof currentPage !== 'undefined') {%>
<nav aria-label="User Pagination">
  <ul class="pagination justify-content-center">
    <!-- Previous Page Link -->
    <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
      <a
        class="page-link"
        href="?search=<%= searchQuery %>&page=<%= currentPage - 1 %>"
        tabindex="-1"
        >Previous</a
      >
    </li>

    <!-- Page Numbers -->
    <% for (let i = 1; i <= totalPages; i++) { %>
    <li class="page-item <%= currentPage === i ? 'active' : '' %>">
      <a
        class="page-link"
        href="?search=<%= searchQuery %>&page=<%= i %>"
        ><%= i %></a
      >
    </li>
    <% } %>

    <!-- Next Page Link -->
    <li
      class="page-item <%= currentPage === totalPages ? 'disabled' : '' %>"
    >
      <a
        class="page-link"
        href="?search=<%= searchQuery %>&page=<%= currentPage + 1 %>"
        >Next</a
      >
    </li>
  </ul>
</nav>

<% } %>
    </div>

    <!-- customers tab content -->
    
    <div class="container" id="customersContent">

      <!-- breadcrumbs -->
     <nav aria-label="breadcrumb" class="my-3">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
        <% if (breadcrumb && breadcrumb.length) { %>
          <% breadcrumb.forEach((crumb, index) => { %>
            <% if (index === breadcrumb.length - 1) { %>
              <li class="breadcrumb-item active" aria-current="page"><%= crumb.name %></li>
            <% } else { %>
              <li class="breadcrumb-item"><a href="<%= crumb.url %>"><%= crumb.name %></a></li>
            <% } %>
          <% }); %>
        <% } %>
      </ol>
    </nav>

      
      <h4 class="my-5">Customers</h4>

      
      <form action="/admin/customers" method="GET">
        <div class="row">
          <div class="col-8">
            <div class="input-group mb-3">
              <input name="search" type="text" class="form-control"
              placeholder="Search..." aria-label="Search" <% if(typeof
              searchQuery!== 'undefined'){ %> value="<%= searchQuery %>" <% } %>
              />
              <button class="btn btn-outline-secondary" type="submit">
                Search
              </button>
            </div>
          </div>
          <div class="col-4 text-end">
            <a href="/admin/add-user" class="btn btn-primary">Add user</a>
          </div>
        </div>
      </form>

      <div class="table-responsive-sm">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">ID</th>
              <th>JOINING DATE</th>
              <th scope="col">NAME</th>
              <th scope="col">EMAIL</th>
              <th scope="col">PHONE</th>
              <th scope="col">STATUS</th>
              <th scope="col">ACTION</th>
            </tr>
          </thead>
          <tbody>
            <% if (typeof users !== 'undefined' && users.length >0) {%> <%
            users.forEach((user)=> {%>

            <tr>
              <td>0000</td>
              <td>
                <%= new Date(user.createdAt).toISOString().split('T')[0] %>
              </td>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.phone %></td>
              <td>Active user (NA)</td>

              <td>
                <a
                  href="/admin/edit-user?Id=<%= user._id %>"
                  class="btn btn-sm mb-1 btn-outline-secondary me-1"
                  title="Edit"
                >
                  <i class="fa-regular fa-pen-to-square"></i>
                </a>

                <!-- block button -->
                <button
                  data-id="<%= user._id %>"
                  class="block-btn btn btn-sm mb-1 btn-outline-danger bg-<%=user.is_blocked==1?'warning':''%> me-1"
                  title="Block"
                >
                  <i class="fa-solid fa-ban"></i>
                </button>

                <!-- delete button -->
                <button
                  data-id="<%= user._id %>"
                  class="delete-btn btn btn-sm mb-1 btn-outline-danger"
                  title="Delete"
                >
                  <i class="fa-solid fa-trash"></i>
                </button>
              </td>
            </tr>

            <% }) %> <% }else{ %>
            <tr>
              <td colspan="6" class="text-center">No user found</td>
            </tr>
            <% } %>
          </tbody>
        </table>

        <!-- Pagination -->

        <% if (typeof currentPage !== 'undefined') {%>
        <nav aria-label="User Pagination">
          <ul class="pagination justify-content-center">
            <!-- Previous Page Link -->
            <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
              <a
                class="page-link"
                href="?search=<%= searchQuery %>&page=<%= currentPage - 1 %>"
                tabindex="-1"
                >Previous</a
              >
            </li>

            <!-- Page Numbers -->
            <% for (let i = 1; i <= totalPages; i++) { %>
            <li class="page-item <%= currentPage === i ? 'active' : '' %>">
              <a
                class="page-link"
                href="?search=<%= searchQuery %>&page=<%= i %>"
                ><%= i %></a
              >
            </li>
            <% } %>

            <!-- Next Page Link -->
            <li
              class="page-item <%= currentPage === totalPages ? 'disabled' : '' %>"
            >
              <a
                class="page-link"
                href="?search=<%= searchQuery %>&page=<%= currentPage + 1 %>"
                >Next</a
              >
            </li>
          </ul>
        </nav>

        <% } %>
      </div>
    </div>

    <!-- Order tab content -->

  <!-- Order tab content -->
<div class="container" id="ordersContent">
  <!-- breadcrumbs -->
  <nav aria-label="breadcrumb" class="my-3">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
      <% if (breadcrumb && breadcrumb.length) { %>
        <% breadcrumb.forEach((crumb, index) => { %>
          <% if (index === breadcrumb.length - 1) { %>
            <li class="breadcrumb-item active" aria-current="page"><%= crumb.name %></li>
          <% } else { %>
            <li class="breadcrumb-item"><a href="<%= crumb.url %>"><%= crumb.name %></a></li>
          <% } %>
        <% }); %>
      <% } %>
    </ol>
  </nav>

  <h4 class="my-5">Orders</h4>

  <form action="/admin/orders" method="GET">
    <div class="row mb-3">
      <div class="col-md-6">
        <div class="input-group">
          <input name="search" type="text" class="form-control" placeholder="Search by Order ID or Customer Name" aria-label="Search" value="<%= typeof searchQuery !== 'undefined' ? searchQuery : '' %>">
          <button class="btn btn-outline-secondary" type="submit">Search</button>
        </div>
      </div>
      <div class="col-md-6">
        <select name="status" class="form-select" onchange="this.form.submit()">
          <option value="">All Statuses</option>
          <option value="Processing" <%= (typeof status !== 'undefined' && status === 'Processing') ? 'selected' : '' %>>Processing</option>
          <option value="Shipped" <%= (typeof status !== 'undefined' && status === 'Shipped') ? 'selected' : '' %>>Shipped</option>
          <option value="Delivered" <%= (typeof status !== 'undefined' && status === 'Delivered') ? 'selected' : '' %>>Delivered</option>
        </select>
      </div>
    </div>
  </form>

  <div class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Order ID</th>
          <th>Customer Name</th>
          <th>Order Date</th>
          <th>Total Amount</th>
          <th>Payment Status</th>
          <th>Delivery Status</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% if (typeof orders !== 'undefined' && orders.length > 0) { %>
          <% orders.forEach(order => { %>
            <tr>
              <td><%= order._id %></td>
              <td><%= order.userId.name %></td>
              <td><%= moment(order.createdAt).format('YYYY-MM-DD HH:mm:ss') %></td>
              <td>₹ <%= order.totalPrice.toFixed(2) %></td>
              <td><span class="badge bg-<%= order.paymentStatus === 'Paid' ? 'success' : 'warning' %>"><%= order.paymentStatus %></span></td>
              <td>
                <select class="form-select form-select-sm delivery-status" data-order-id="<%= order._id %>">
                  <option value="Processing" <%= order.deliveryStatus === 'Processing' ? 'selected' : '' %>>Processing</option>
                  <option value="Shipped" <%= order.deliveryStatus === 'Shipped' ? 'selected' : '' %>>Shipped</option>
                  <option value="Delivered" <%= order.deliveryStatus === 'Delivered' ? 'selected' : '' %>>Delivered</option>
                </select>
              </td>
              <td>
                <button class="btn btn-sm btn-primary view-order" data-order-id="<%= order._id %>">View</button>
              </td>
            </tr>
          <% }); %>
        <% } else { %>
          <tr>
            <td colspan="7" class="text-center">No orders found</td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>

  <!-- Pagination -->
  <% if (typeof currentPage !== 'undefined' && typeof totalPages !== 'undefined') { %>
    <nav aria-label="Order Pagination">
      <ul class="pagination justify-content-center">
        <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
          <a class="page-link" href="?page=<%= currentPage - 1 %>&search=<%= searchQuery %>&status=<%= typeof status !== 'undefined' ? status : '' %>" tabindex="-1">Previous</a>
        </li>
        <% for (let i = 1; i <= totalPages; i++) { %>
          <li class="page-item <%= currentPage === i ? 'active' : '' %>">
            <a class="page-link" href="?page=<%= i %>&search=<%= searchQuery %>&status=<%= typeof status !== 'undefined' ? status : '' %>"><%= i %></a>
          </li>
        <% } %>
        <li class="page-item <%= currentPage === totalPages ? 'disabled' : '' %>">
          <a class="page-link" href="?page=<%= currentPage + 1 %>&search=<%= searchQuery %>&status=<%= typeof status !== 'undefined' ? status : '' %>">Next</a>
        </li>
      </ul>
    </nav>
  <% } %>
</div>

<!-- Order Details Modal -->
<div class="modal fade" id="orderDetailsModal" tabindex="-1" aria-labelledby="orderDetailsModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="orderDetailsModalLabel">Order Details</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body" id="orderDetailsContent">
        <!-- Order details will be loaded here -->
      </div>
    </div>
  </div>
</div>
  </div>
</main>

